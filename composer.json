{
    "name": "tomatophp/filament-tenancy",
    "type": "library",
    "description": "Tenancy multi-database integration for FilamentPHP",
    "keywords": [
        "php",
        "laravel",
        "tenancy",
        "multi-database",
        "multi-tenancy",
        "filamentphp",
        "filament-plugins",
        "tomatophp"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentTenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentTenancy\\FilamentTenancyServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "stancl/tenancy": "^3.8",
        "tomatophp/console-helpers": "^1.1",
        "filament/filament": "^3.0"
    }
}
